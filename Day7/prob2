#include <iostream>
#include <vector>
using namespace std;

vector<long long> productExceptSelf(vector<int>& nums) {
    int n = nums.size();
    vector<long long> ans(n);

    long long prefix = 1;

    // Step 1: prefix product
    for (int i = 0; i < n; i++) {
        ans[i] = prefix;
        prefix *= nums[i];
    }

    long long suffix = 1;

    // Step 2: multiply suffix product from right
    for (int i = n - 1; i >= 0; i--) {
        ans[i] *= suffix;
        suffix *= nums[i];
    }

    return ans;
}

int main() {
    vector<int> arr = {1, 2, 3, 4};
    vector<long long> res = productExceptSelf(arr);

    for (long long x : res)
        cout << x << " ";
}
