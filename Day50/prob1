#include <iostream>
#include <vector>
using namespace std;

void rotate180(vector<vector<int>>& mat) {
    int n = mat.size();
    int m = mat[0].size();

    // Reverse the rows order
    reverse(mat.begin(), mat.end());

    // Reverse every row individually
    for(int i = 0; i < n; i++) {
        reverse(mat[i].begin(), mat[i].end());
    }
}

int main() {
    int n, m;
    cout << "Enter rows and columns: ";
    cin >> n >> m;

    vector<vector<int>> mat(n, vector<int>(m));

    cout << "Enter matrix:\n";
    for(int i = 0; i < n; i++)
        for(int j = 0; j < m; j++)
            cin >> mat[i][j];

    rotate180(mat);

    cout << "\nMatrix after 180 degree rotation:\n";
    for(int i = 0; i < n; i++) {
        for(int j = 0; j < m; j++)
            cout << mat[i][j] << " ";
        cout << endl;
    }

    return 0;
}
