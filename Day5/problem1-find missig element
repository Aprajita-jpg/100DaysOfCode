#include <iostream>
#include <vector>
using namespace std;

int findMissing(vector<int> &arr, int n) {
    int total = n * (n + 1) / 2;
    int sum = 0;

    for(int x : arr) sum += x;

    return total - sum;
}

int main() {
    vector<int> arr = {1, 2, 4, 5};
    int n = 5;

    cout << "Missing number: " << findMissing(arr, n);
}
