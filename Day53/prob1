#include <iostream>
#include <vector>
using namespace std;

void maxInEachRow(vector<vector<int>>& mat) {
    int n = mat.size();
    int m = mat[0].size();

    for (int i = 0; i < n; i++) {
        int rowMax = mat[i][0];
        for (int j = 1; j < m; j++) {
            if (mat[i][j] > rowMax)
                rowMax = mat[i][j];
        }
        cout << "Maximum in row " << i << " = " << rowMax << endl;
    }
}

int main() {
    int n, m;
    cout << "Enter rows and columns: ";
    cin >> n >> m;

    vector<vector<int>> mat(n, vector<int>(m));
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> mat[i][j];

    maxInEachRow(mat);

    return 0;
}
