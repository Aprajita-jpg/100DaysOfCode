#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

// Function to find intersection point
Node* getIntersectionNode(Node* headA, Node* headB) {
    if (!headA || !headB) return NULL;

    Node* p1 = headA;
    Node* p2 = headB;

    while (p1 != p2) {
        p1 = (p1 == NULL) ? headB : p1->next;
        p2 = (p2 == NULL) ? headA : p2->next;
    }

    return p1;   // intersection node or NULL
}
