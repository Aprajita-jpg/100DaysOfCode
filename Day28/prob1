#include <iostream>
using namespace std;

int main() {
    string s = "leetcode";
    int freq[256] = {0};

    // Count frequency of each character
    for (char c : s) {
        freq[c]++;
    }

    // Find first character with freq = 1
    for (char c : s) {
        if (freq[c] == 1) {
            cout << "First unique character: " << c;
            return 0;
        }
    }

    cout << "No unique character found";
    return 0;
}
