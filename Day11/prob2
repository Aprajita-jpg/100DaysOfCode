#include <bits/stdc++.h>
using namespace std;

vector<int> mergeTwoSortedArrays(vector<int>& a, vector<int>& b) {
    int n = a.size(), m = b.size();
    vector<int> result;
    result.reserve(n + m);

    int i = 0, j = 0;

    while (i < n && j < m) {
        if (a[i] <= b[j]) {
            result.push_back(a[i]);
            i++;
        } else {
            result.push_back(b[j]);
            j++;
        }
    }

    // Copy remaining elements
    while (i < n) result.push_back(a[i++]);
    while (j < m) result.push_back(b[j++]);

    return result;
}

int main() {
    vector<int> a = {1, 3, 5, 7};
    vector<int> b = {2, 4, 6, 8};

    vector<int> merged = mergeTwoSortedArrays(a, b);

    for (int x : merged) cout << x << " ";
}
