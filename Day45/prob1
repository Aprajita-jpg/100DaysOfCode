#include <iostream>
#include <vector>
using namespace std;

bool searchMatrix(vector<vector<int>>& mat, int target) {
    int rows = mat.size();
    int cols = mat[0].size();

    int r = 0;          // start at top-right
    int c = cols - 1;

    while (r < rows && c >= 0) {
        if (mat[r][c] == target)
            return true;

        if (mat[r][c] > target)
            c--;        // move left
        else
            r++;        // move down
    }

    return false;
}

int main() {
    vector<vector<int>> mat = {
        {1, 4, 7, 11},
        {2, 5, 8, 12},
        {3, 6, 9, 16},
        {10, 13, 14, 17}
    };

    int target = 9;

    if (searchMatrix(mat, target))
        cout << "Found";
    else
        cout << "Not Found";

    return 0;
}
