#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

// Function to find nth node from end
Node* findNthFromEnd(Node* head, int n) {
    int len = 0;
    Node* temp = head;
    while (temp) {
        len++;
        temp = temp->next;
    }

    if (n > len) return NULL;

    temp = head;
    for (int i = 0; i < len - n; i++) {
        temp = temp->next;
    }

    return temp;
}

// Insert at end
void insert(Node*& head, int val) {
    Node* newNode = new Node();
    newNode->data = val;
    newNode->next = NULL;

    if (!head) {
        head = newNode;
        return;
    }

    Node* temp = head;
    while (temp->next)
        temp = temp->next;

    temp->next = newNode;
}

// Print list
void printList(Node* head) {
    while (head) {
        cout << head->data << " -> ";
        head = head->next;
    }
    cout << "NULL";
}

int main() {
    Node* head = NULL;
    insert(head, 1);
    insert(head, 2);
    insert(head, 3);
    insert(head, 4);
    insert(head, 5);

    int n = 2;
    Node* node = findNthFromEnd(head, n);
    if (node)
        cout << "Nth node from end: " << node->data << endl;
    else
        cout << "Node does not exist" << endl;

    return 0;
}
