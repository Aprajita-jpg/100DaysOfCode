#include <iostream>
using namespace std;

int main() {
    string s1 = "apple";
    string s2 = "banana";

    int result = s1.compare(s2);

    if (result == 0) {
        cout << "Strings are equal";
    }
    else if (result < 0) {
        cout << "s1 is smaller than s2";
    }
    else {
        cout << "s1 is greater than s2";
    }

    return 0;
}
