#include <iostream>
#include <vector>
using namespace std;

bool isSymmetric(vector<vector<int>>& mat) {
    int n = mat.size();

    // Only square matrices can be symmetric
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            if (mat[i][j] != mat[j][i])
                return false;
        }
    }
    return true;
}

int main() {
    int n;
    cout << "Enter size of square matrix: ";
    cin >> n;

    vector<vector<int>> mat(n, vector<int>(n));
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> mat[i][j];

    if (isSymmetric(mat))
        cout << "Matrix is symmetric";
    else
        cout << "Matrix is not symmetric";

    return 0;
}
